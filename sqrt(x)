int mySqrt(int x) {
    if(x==0||x==1){
        return x;}
        long low = 0,high = x,ans = 0,mid;
        while(low<=high){
            mid = low + (high-low)/2;
            if(mid*mid==x){
                return mid;
            }
            else if(mid*mid<x){
                ans = mid;
                low = mid + 1;
            }
            else{
                high = mid - 1;
            }

        }
        return ans;
}
